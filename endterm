CREATE TABLE tableName (
    sex	VARCHAR(512),
    Medu	INT,
    Fedu	INT,
    guardian	VARCHAR(512),
    traveltime	VARCHAR(512),
    studytime	INT,
    failures	VARCHAR(512),
    schoolsup	VARCHAR(512),
    famsup	VARCHAR(512),
    romantic	VARCHAR(512),
    famrel	INT,
    freetime	INT,
    absences	INT,
    final	INT
);

