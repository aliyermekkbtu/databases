CREATE TABLE tableName (
    id INT,
    Date VARCHAR(512),
    Year INT,
    Month VARCHAR(512),
    CustomerAge INT,
    CustomerGender VARCHAR(512),
    Country VARCHAR(512),
    State VARCHAR(512),
    ProductCategory VARCHAR(512),
    SubCategory VARCHAR(512),
    Quantity INT,
    UnitCost DECIMAL(10, 2),
    UnitPrice DECIMAL(10, 2),
    Cost DECIMAL(10, 2),
    Revenue DECIMAL(10, 2),
    Column1 VARCHAR(512)
);

